			{
				type =  form
				form = QR_Scan
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						// this_user_Jobs_date = "CN00002|YYhhdhe||12-Jul-2023";
										this_user_Jobs_date = input.User_ID;
										jobs_dates = this_user_Jobs_date.getSuffix("||");
										jobs_date_formate = toDate(jobs_dates,"dd-MMM-yyyy");
										user_ids = this_user_Jobs_date.getPrefix("|");
										start_s = this_user_Jobs_date.find("|");
										end_s = this_user_Jobs_date.find("||");
										user_jobs = this_user_Jobs_date.mid(start_s + 1,end_s);
										this_user_jobs = USER_JOBS[User.User_ID == user_ids && Jobs.Job_Name == user_jobs && Jobs.Visit_Date == jobs_date_formate];
										// info this_user_jobs.ID;
										// alert this_user_jobs.ID;
										if(this_user_jobs.Jobs_Progress == "Scheduled" && this_user_jobs.Status == "Applied")
										{
											if(this_user_jobs.Jobs.Visit_Date == zoho.currentdate)
											{
												this_user_jobs.Jobs_Progress="Reported";
												this_user_jobs.Attendence_Time=zoho.currenttime;
												alert "Attendence Successfull";
											}
											else
											{
												alert "Today is not Job Visit date";
											}
										}
										else if(this_user_jobs.Jobs_Progress == "Reported" && this_user_jobs.Status == "Applied")
										{
											alert "Already Reported";
										}
										else if(this_user_jobs.Jobs_Progress == "Accepted" && this_user_jobs.Status == "Applied")
										{
											alert "Not Scheduled Yet only Accepted";
										}
										else if(this_user_jobs.Jobs_Progress == "Pending" && this_user_jobs.Status == "Applied")
										{
											alert "User is Pending";
										}
										else if(this_user_jobs.Jobs_Progress == "Rejected" && this_user_jobs.Status == "Applied")
										{
											alert "Rejected user";
										}
										else
										{
											alert "Not found";
										}
					)
					}
				}
			}
