		list Applied_Jobs
		{
			displayName = "Applied Jobs"
			print template = PASS
			show all rows from USER_JOBS    
			(
				User
				Name
				Jobs
				Applied_Date as "Applied Date"
				Status
				custom action "Cancel"
				(
	  			workflow = Cancel_User_Jobs
				show action in edit record menu = true
				column header = "Cancel"
				condition = (Status == "Applied" && Jobs_Progress == "Pending" || Jobs_Progress == "Rejected" || Jobs_Progress == "Accepted")
				confirmation before execution
				(
					message for individual record = "Do you want to cancel?"
					confirm button text = "Yes"		
					cancel button text = "No"
				)
				)
				Jobs.Visit_Date as "Visit Date"
				Jobs.Income
				Jobs_Progress as "Jobs Progress"
				Reporting_Time as "Reporting Time"
				custom action "Download"
				(
	  			workflow = Download_pass_for_users_f
				show action in edit record menu = true
				column header = "Download"
				condition = (Status == "Applied" && Jobs_Progress == "Scheduled")
				)
				User.User_ID as "User ID"
			)
			conditional formatting
			(
				"Scheduled"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Scheduled" && Reporting_Time is not null)
				fields = [Reporting_Time]
				format = "color:#ffffff;background-color:#3598db;"
				}
				"Scheduled"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Scheduled")
				fields = [Jobs_Progress]
				format = "color:#ffffff;background-color:#5dc35a;"
				}
				"Rejected"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Rejected")
				fields = [Name, Jobs]
				format = "text-decoration:line-through;color:#e64d3b;"
				}
				"Pending"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Pending")
				fields = [Jobs_Progress]
				format = "color:#ffffff;background-color:#1bbc9b;"
				}
				"Accepted"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Accepted")
				fields = [Jobs_Progress]
				format = "color:#ffffff;background-color:#765f89;"
				}
				"Rejected"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Rejected")
				fields = [Jobs_Progress]
				format = "color:#ffffff;background-color:#e84c3d;"
				}
				"Reporting Expect Scheduled"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Pending" || Jobs_Progress == "Rejected" || Jobs_Progress == "Accepted" || Jobs_Progress == "Reported")
				fields = [Reporting_Time]
				format = "color:#ffffff;background-color:#efb116;"
				}
				"Reported"
				{
				condition = (Status == "Applied" && Jobs_Progress == "Reported")
				fields = [Jobs_Progress]
				format = "color:#ffffff;background-color:#efb116;"
				}
				"after scheduled not visit"
				{
				condition = (Jobs_Progress == "Scheduled" && Jobs.Visit_Date < zoho.currentdate)
				fields = [Name, Jobs, Applied_Date, Jobs.Visit_Date, Jobs.Income, Jobs_Progress, Reporting_Time, User.User_ID]
				format = "text-decoration:line-through;color:#000000;background-color:#ffffff;"
				}
			)
			quickview
			(
				layout
				(
		 			type = -1
					datablock1
					(
			 			layout type = -1
						fields
						(
				"Cancel"
				"Download" as "Attendence Pass"
							User.User_ID as "User ID"
