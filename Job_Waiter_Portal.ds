	 	 Content="<zml    \t\t\n\t\t\n\t\t\n\t\t\twebDeviceConfig='{\"layout\":{\"displayType\":\"plain\",\"design\":\"fluid\",\"style\":\"padding-right:0px;padding-top:0px;padding-bottom:0px;padding-left:0px;\"}}'\n\t\t\n\t\t\n>\n\t<layout>\n\t<row>\n\t<column\n   \t\t width=\"100%\"\n   \t>\n\t<dsp id='html_snippet' elementName=\"HTML Snippet\"\n bgColor=\"#FFFFFF\" \n>\n\t<![CDATA[htmlpage html_snippet(p1,p2,p3)\ncontent\n&lt;%{\n\t\/\/ Define a map of stages that maps stage names to their respective values\n\tstages = Parameters[Parameter == &quot;TXT_REG_STAGES&quot;].Value.toMap();\n\t\/\/ Get the value of the &quot;p1&quot; parameter to determine which stage to display\n\ttab = ifnull(p1,stages.get(&quot;Register&quot;));\n\t\/\/ If the current stage is not &quot;Register&quot;, retrieve information about the current user from the database\n\tif(tab == stages.get(&quot;Dashboard&quot;) || tab == stages.get(&quot;User&quot;) || tab == stages.get(&quot;AllJobs&quot;) || tab == stages.get(&quot;JobsApplied&quot;) || tab == stages.get(&quot;JobAppliedAccepted&quot;) || tab == stages.get(&quot;JobAppliedScheduled&quot;) || tab == stages.get(&quot;JobAppliedReported&quot;))\n\t{\n\t\t\/\/ Retrieve user profile information from the database\n\t\tthis_user = User_Profile[ID == p2.toNumber()];\n\t\tthis_user.Login_status=true;\n\t}\n\t\/\/ \t\telse\n\t\/\/ \t\t{\n\t\/\/ \t\t\t\/\/ Retrieve information about the user attempting to register from the database\n\t\/\/ \t\t\tthis_user.Login_status=false;\n\t\/\/ \t\t}\n\t\/\/ Generate HTML tags to display the appropriate web page or iframe based on the value of &quot;p1&quot;\n\tif(tab == stages.get(&quot;Register&quot;))\n\t{\n\t\tURL_EM_LOGIN_PAGE = Parameters[Parameter == &quot;URL_EM_LOGIN_PAGE&quot;].Value.toString();\n\t\tnext_stage = stages.get(&quot;Login&quot;);\n\t\tURL_EM_REGISTER = Parameters[Parameter == &quot;URL_EM_REGISTER&quot;].Value.toString();\n\t\t%&gt;\n&lt;!doctype html&gt;\n&lt;html lang=&quot;en&quot;&g