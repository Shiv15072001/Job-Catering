										filename = "Attendence_Pass";
										url_edit = Parameters[Parameter == "URL_EM_JOBSAPPLIED"].Value.toString();
										url_edit = url_edit.replaceAll("report-embed","record-pdf");
										split_point = url_edit.lastIndexOf("/");
